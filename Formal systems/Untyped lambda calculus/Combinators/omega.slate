/**
 * @references * https://en.wikipedia.org/wiki/Lambda_calculus#Standard_terms
 */
$omega: %ExplicitOperator {
  display = ['𝞈'],
  definition = [$../Terms(V = {$../../../Essentials/Sets/"Empty set"}).abstraction(x = {
      #(x: %Element($../../Variables)),
      {M = {$../Terms(V = {%enumeration(x)}).application(
          M = {x},
          N = {x}
      )}}
  })]
}
