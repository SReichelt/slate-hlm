%../../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Lambda_calculus#Standard_terms
 */
$constant: %ExplicitOperator {
  display = ['𝗞'],
  definition = [$../Terms(V = {$../../../Essentials/Sets/"Empty set"}).abstraction(x = {
      #(x: %Element($../../Variables)),
      {M = {$../Terms(V = {$../../../Essentials/Sets/"Empty set"}).abstraction(x = {
          #(y: %Element($../../"Fresh variables"(V = {%enumeration(x)}))),
          {M = {x}}
      })}}
  })]
}
