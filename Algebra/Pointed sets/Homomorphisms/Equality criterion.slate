%../../../../../logics/hlm%

/**
 * @remarks This follows immediately from the HLM-specific definition of equality of pointed sets.
 */
$"Equality criterion"(
  ğ’: %Element($../"Pointed sets"),
  ğ“: %Element(%previous)
): %EquivalenceTheorem {
  conditions = [
    %not($../../../Essentials/Sets/empty(S = {$Isomorphisms(
      ğ’ = {ğ’},
      ğ“ = {ğ“}
    )})),
    %equals(ğ’, ğ“)
  ]
}
