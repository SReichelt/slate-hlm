$"subset of pointed set"(
  ğ’: %Element($../"Pointed sets"),
  S: %Subset($../Carrier(ğ’ = ğ’))
): %Predicate {
  notation = $NounRelation(
    operands = [S, ğ’],
    singular = 'pointed subset',
    plural = 'pointed subsets'
  ),
  definition = [%in(
    $../"base point"(ğ’ = ğ’),
    S
  )]
}
