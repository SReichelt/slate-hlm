%../../../../../logics/hlm%

$"subset as pointed set"(
  ğ’: %Element(
    $../"Pointed sets",
    auto = %true
  ),
  S: %Subset($../Carrier(ğ’ = {ğ’})),
  _1: %Constraint($"subset of pointed set"(
    ğ’ = {ğ’},
    S = {S}
  ))
): %ExplicitOperator {
  display = [$Parens(
    body = S,
    style = '[]'
  )],
  definition = [$../"Pointed sets"."pointed set"(
    S = {S},
    s = {$../"base point"(ğ’ = {ğ’})}
  )]
}
