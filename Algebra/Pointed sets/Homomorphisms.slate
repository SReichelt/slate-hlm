%../../../../logics/hlm%

$Homomorphisms(
  𝐒: %Element($"Pointed sets"),
  𝐓: %Element(%previous)
): %SetOperator {
  display = [$Function(
    function = 'Hom',
    arguments = [𝐒, 𝐓]
  )],
  definitionDisplay = {
    parameter = #(φ: %Expr),
    display = [$FunctionDefinition(
      function = φ,
      symbol = '→',
      operands = [𝐒, 𝐓]
    )],
    singularName = 'homomorphism',
    pluralName = 'homomorphisms'
  },
  definition = [%subset(
    #(f: %Element($../../Essentials/Functions/Functions(
      X = {$Carrier(𝐒 = {𝐒})},
      Y = {$Carrier(𝐒 = {𝐓})}
    ))),
    %equals(
      $../../Essentials/Functions/value(
        X = {$Carrier(𝐒 = {𝐒})},
        Y = {$Carrier(𝐒 = {𝐓})},
        f = {f},
        x = {$"base point"(𝐒 = {𝐒})}
      ),
      $"base point"(𝐒 = {𝐓})
    )
  )]
}
