%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Identity_is_Unique
 */
$identity(
  𝐌: %Element($Magmas),
  _1: %Constraint(%exists(
    #(e: %Element($Carrier(𝐌 = 𝐌))),
    formula = $"identity element"(𝐌 = 𝐌, e = e)
  ))
): %ImplicitOperator {
  notation = $SubSup(body = '1', sub = 𝐌),
  parameter = #(m: %Element($Carrier(𝐌 = 𝐌))),
  definition = [$"identity element"(𝐌 = 𝐌, e = m)]
}
