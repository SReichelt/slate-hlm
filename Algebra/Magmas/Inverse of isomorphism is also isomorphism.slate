%../../../../logics/hlm%

$"Inverse of isomorphism is also isomorphism"(
  𝐌: %Element($Magmas),
  𝐍: %Element(%previous),
  φ: %Element($Isomorphisms(
    𝐌 = {𝐌},
    𝐍 = {𝐍}
  ))
): %StandardTheorem {
  claim = %in(
    $../../Essentials/Functions/inverse(
      X = {$Carrier(𝐌 = {𝐌})},
      Y = {$Carrier(𝐌 = {𝐍})},
      f = {φ}
    ),
    $Isomorphisms(
      𝐌 = {𝐍},
      𝐍 = {𝐌}
    )
  )
}
