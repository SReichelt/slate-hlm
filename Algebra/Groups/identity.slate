%../../../../logics/hlm%

$identity(𝐆: %Element($Groups)): %ExplicitOperator {
  display = [$SubSup(
    body = 'e',
    sub = 𝐆
  )],
  definition = [%structuralCases(
    𝐆,
    $Groups,
    [{
      constructor = $Groups.group,
      parameters = #(
        G: %Set,
        ∗: %Element($../../Essentials/Functions/Operations/"Inner operations"(X = {G})),
        e: %Element(G),
        i: %Element($../../Essentials/Functions/Functions(
          X = {G},
          Y = {G}
        )),
        _1: %Constraint($group(
          G = {G},
          ∗ = {∗},
          e = {e},
          i = {i}
        ))
      ),
      value = e
    }]
  )]
}
