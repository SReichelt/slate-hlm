%../../../../../logics/hlm%

$"Inverse of isomorphism is also isomorphism"(
  𝐆,𝐇: %Element($../Groups),
  φ: %Element($Isomorphisms(
    𝐆 = {𝐆},
    𝐇 = {𝐇}
  ))
): %StandardTheorem {
  claim = %in(
    $../../../Essentials/Functions/inverse(
      X = {$../Carrier(𝐆 = {𝐆})},
      Y = {$../Carrier(𝐆 = {𝐇})},
      f = {φ}
    ),
    $Isomorphisms(
      𝐆 = {𝐇},
      𝐇 = {𝐆}
    )
  )
}
