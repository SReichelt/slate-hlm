%../../../../../logics/hlm%

/**
 * @references * https://leanprover-community.github.io/mathlib_docs/group_theory/subgroup.html#subtype.group
 */
$"subset group"(
  𝐆: %Element($../Groups),
  S: %Subset($../Carrier(𝐆 = {𝐆})),
  _1: %Constraint($"subgroup set"(
    𝐆 = {𝐆},
    S = {S}
  ))
): %ExplicitOperator {
  notation = $SubSup(
    body = $Parens(
      body = S,
      style = '[]'
    ),
    sub = 𝐆
  ),
  definition = [%structuralCases(
    𝐆,
    $../Groups,
    [{
      constructor = $../Groups.group,
      parameters = #(
        G: %Set,
        ∗: %Element($../../../Essentials/Functions/Operations/"Inner operations"(X = {G})),
        e: %Element(G),
        i: %Element($../../../Essentials/Functions/Functions(
          X = {G},
          Y = {G}
        )),
        _1: %Constraint($../group(
          G = {G},
          ∗ = {∗},
          e = {e},
          i = {i}
        ))
      ),
      value = $../Groups.group(
        G = {S},
        ∗ = {$../../../Essentials/Functions/Operations/"inner operation restriction"(
          X = {G},
          A = {S},
          ∗ = {∗}
        )},
        e = {e},
        i = {$../../../Essentials/Functions/"codomain restriction"(
          X = {G},
          A = {S},
          Y = {G},
          B = {S},
          f = {i}
        )}
      )
    }]
  )]
}
