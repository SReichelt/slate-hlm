%../../../../../logics/hlm%

$finite(𝐆: %Element($../Groups)): %Predicate {
  display = [$Property(
    operand = 𝐆,
    property = %neg('finite', 'infinite')
  )],
  definition = [
    $../../../Essentials/Numbers/Cardinal/finite(κ = {$"group order"(𝐆 = {𝐆})}),
    $../../../Essentials/Sets/finite(S = {$../Carrier(𝐆 = {𝐆})})
  ]
}
