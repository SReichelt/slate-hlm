$"Lagranges corollary"(
  𝐆: %Element($../Groups),
  _1: %Constraint($../finite(𝐆 = {𝐆})),
  g: %Element($../Carrier(𝐆 = {𝐆}))
): %StandardTheorem {
  claim = $../../../Essentials/Numbers/Natural/divisible(
    m = {$"element order"(
      𝐆 = {𝐆},
      g = {g}
    )},
    n = {$"group order"(𝐆 = {𝐆})}
  )
}
