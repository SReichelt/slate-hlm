%../../../../../logics/hlm%

$"Groups with order"(κ: %Element($../../../Essentials/Numbers/Cardinal/"Cardinal numbers")): %SetOperator {
  display = [$SubSup(
    body = '𝓖𝓻𝓹',
    sub = κ
  )],
  definitionDisplay = {
    parameter = #(𝐆: %Expr),
    singularName = ['group of order', ' ', κ],
    pluralName = ['groups of order', ' ', κ]
  },
  definition = [%subset(
    #(𝐆: %Element($../Groups)),
    %equals(
      $"group order"(𝐆 = {𝐆}),
      κ
    )
  )]
}
