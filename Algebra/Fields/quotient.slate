%../../../../logics/hlm%

[
  $~Carrier = $../Rings/Carrier,
  $~Rings = $../Rings
]

$quotient(
  ğ…: %Element($Fields),
  b: %Element($~Carrier(ğ‘ = ğ…)),
  a: %Element($~Rings/Units(ğ‘ = ğ…))
): %ImplicitOperator {
  notation = $Fraction(numerator = b, denominator = a),
  parameter = #(x: %Element($~Carrier(ğ‘ = ğ…))),
  definition = [%equals(
    b,
    $~Rings/product(ğ‘ = ğ…, a = x, b = a)
  )]
}
