%../../../../../logics/hlm%

$Homomorphisms(
  𝐌: %Element($../Monoids),
  𝐍: %Element(%previous)
): %SetOperator {
  display = [$Function(
    function = 'Hom',
    arguments = [𝐌, 𝐍]
  )],
  definitionDisplay = {
    parameter = #(φ: %Expr),
    display = [$FunctionDefinition(
      function = φ,
      symbol = '→',
      operands = [𝐌, 𝐍]
    )],
    singularName = 'homomorphism',
    pluralName = 'homomorphisms'
  },
  definition = [%subset(
    #(f: %Element($../../../Essentials/Functions/Functions(
      X = {$../Carrier(𝐌 = {𝐌})},
      Y = {$../Carrier(𝐌 = {𝐍})}
    ))),
    %and(
      %forall(
        #(
          a: %Element($../Carrier(𝐌 = {𝐌})),
          b: %Element(%previous)
        ),
        %equals(
          $../../../Essentials/Functions/value(
            X = {$../Carrier(𝐌 = {𝐌})},
            Y = {$../Carrier(𝐌 = {𝐍})},
            f = {f},
            x = {$../operation(
              𝐌 = {𝐌},
              a = {a},
              b = {b}
            )}
          ),
          $../operation(
            𝐌 = {𝐍},
            a = {$../../../Essentials/Functions/value(
              X = {$../Carrier(𝐌 = {𝐌})},
              Y = {$../Carrier(𝐌 = {𝐍})},
              f = {f},
              x = {a}
            )},
            b = {$../../../Essentials/Functions/value(
              X = {$../Carrier(𝐌 = {𝐌})},
              Y = {$../Carrier(𝐌 = {𝐍})},
              f = {f},
              x = {b}
            )}
          )
        )
      ),
      %equals(
        $../../../Essentials/Functions/value(
          X = {$../Carrier(𝐌 = {𝐌})},
          Y = {$../Carrier(𝐌 = {𝐍})},
          f = {f},
          x = {$../identity(𝐌 = {𝐌})}
        ),
        $../identity(𝐌 = {𝐍})
      )
    )
  )]
}
