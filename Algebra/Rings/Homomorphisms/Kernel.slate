%../../../../../logics/hlm%

$Kernel(
  𝐑,𝐒: %Element(
    $../Rings,
    auto = %true
  ),
  φ: %Element($Homomorphisms(
    𝐑 = {𝐑},
    𝐒 = {𝐒}
  ))
): %SetOperator {
  notation = $Function(
    function = 'Ker',
    arguments = [φ]
  ),
  definition = [$../../../Essentials/Functions/Preimage(
    X = {$../Carrier(𝐑 = {𝐑})},
    Y = {$../Carrier(𝐑 = {𝐒})},
    f = {φ},
    S = {%enumeration($../zero(𝐑 = {𝐒}))}
  )]
}
