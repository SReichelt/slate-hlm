%../../../../../logics/hlm%

$Isomorphisms(
  𝐑: %Element($../Rings),
  𝐒: %Element(%previous)
): %SetOperator {
  notation = [$Function(
    function = 'Iso',
    arguments = [𝐑, 𝐒]
  )],
  definitionNotation = {
    parameter = #(φ: %Expr),
    notation = [$FunctionDeclaration(
      function = φ,
      symbol = '↔',
      operands = [𝐑, 𝐒]
    )],
    singularName = 'isomorphism',
    pluralName = 'isomorphisms'
  },
  definition = [
    %subset(
      #(φ: %Element($Homomorphisms(
        𝐑 = {𝐑},
        𝐒 = {𝐒}
      ))),
      $../../../Essentials/Functions/bijective(
        X = {$../Carrier(𝐑 = {𝐑})},
        Y = {$../Carrier(𝐑 = {𝐒})},
        f = {φ}
      )
    ),
    %subset(
      #(f: %Element($../../../Essentials/Functions/Bijections(
        X = {$../Carrier(𝐑 = {𝐑})},
        Y = {$../Carrier(𝐑 = {𝐒})}
      ))),
      %and(
        %forall(
          #(
            a: %Element($../Carrier(𝐑 = {𝐑})),
            b: %Element(%previous)
          ),
          %equals(
            $../../../Essentials/Functions/value(
              X = {$../Carrier(𝐑 = {𝐑})},
              Y = {$../Carrier(𝐑 = {𝐒})},
              f = {f},
              x = {$../sum(
                𝐑 = {𝐑},
                a = {a},
                b = {b}
              )}
            ),
            $../sum(
              𝐑 = {𝐒},
              a = {$../../../Essentials/Functions/value(
                X = {$../Carrier(𝐑 = {𝐑})},
                Y = {$../Carrier(𝐑 = {𝐒})},
                f = {f},
                x = {a}
              )},
              b = {$../../../Essentials/Functions/value(
                X = {$../Carrier(𝐑 = {𝐑})},
                Y = {$../Carrier(𝐑 = {𝐒})},
                f = {f},
                x = {b}
              )}
            )
          )
        ),
        %forall(
          #(
            d: %Element($../Carrier(𝐑 = {𝐑})),
            e: %Element(%previous)
          ),
          %equals(
            $../../../Essentials/Functions/value(
              X = {$../Carrier(𝐑 = {𝐑})},
              Y = {$../Carrier(𝐑 = {𝐒})},
              f = {f},
              x = {$../product(
                𝐑 = {𝐑},
                a = {d},
                b = {e}
              )}
            ),
            $../product(
              𝐑 = {𝐒},
              a = {$../../../Essentials/Functions/value(
                X = {$../Carrier(𝐑 = {𝐑})},
                Y = {$../Carrier(𝐑 = {𝐒})},
                f = {f},
                x = {d}
              )},
              b = {$../../../Essentials/Functions/value(
                X = {$../Carrier(𝐑 = {𝐑})},
                Y = {$../Carrier(𝐑 = {𝐒})},
                f = {f},
                x = {e}
              )}
            )
          )
        ),
        %equals(
          $../../../Essentials/Functions/value(
            X = {$../Carrier(𝐑 = {𝐑})},
            Y = {$../Carrier(𝐑 = {𝐒})},
            f = {f},
            x = {$../one(𝐑 = {𝐑})}
          ),
          $../one(𝐑 = {𝐒})
        )
      )
    )
  ]
}
