%../../../../../logics/hlm%

$"Inverse of isomorphism is also isomorphism"(
  𝐑: %Element($../Rings),
  𝐒: %Element(%previous),
  φ: %Element($Isomorphisms(
    𝐑 = {𝐑},
    𝐒 = {𝐒}
  ))
): %StandardTheorem {
  claim = %in(
    $../../../Essentials/Functions/inverse(
      X = {$../Carrier(𝐑 = {𝐑})},
      Y = {$../Carrier(𝐑 = {𝐒})},
      f = {φ}
    ),
    $Isomorphisms(
      𝐑 = {𝐒},
      𝐒 = {𝐑}
    )
  )
}
