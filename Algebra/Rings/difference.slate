%../../../../logics/hlm%

$difference(
  ğ‘: %Element(
    $Rings,
    auto = %true
  ),
  a: %Element($Carrier(ğ‘ = {ğ‘})),
  b: %Element(%previous)
): %ExplicitOperator {
  notation = [$SubtractionOperator(operands = [a, b])],
  definition = [$sum(
    ğ‘ = {ğ‘},
    a = {a},
    b = {$"additive inverse"(
      ğ‘ = {ğ‘},
      a = {b}
    )}
  )]
}
