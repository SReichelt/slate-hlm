%../../../../../../logics/hlm%

$"Identity matrix of dimension 2"(𝐑: %Element($../../Rings)): %StandardTheorem {
  claim = %equals(
    $../identity(
      𝐑 = {𝐑},
      n = {$../../../../Essentials/Numbers/Natural/number(value = 2)}
    ),
    $../matrix(
      𝐑 = {𝐑},
      m = 2,
      n = 2,
      items = [
        [
          {$../../one(𝐑 = {𝐑})},
          {$../../zero(𝐑 = {𝐑})}
        ],
        [
          {$../../zero(𝐑 = {𝐑})},
          {$../../one(𝐑 = {𝐑})}
        ]
      ]
    )
  )
}
