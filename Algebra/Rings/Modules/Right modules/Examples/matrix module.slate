%../../../../../../../logics/hlm%

$"matrix module"(
  𝐑: %Element($../../../Rings),
  m: %Element($../../../../../Essentials/Numbers/Natural/"Natural numbers"),
  n: %Element(%previous)
): %ExplicitOperator {
  notation = $SubSup(
    body = 𝐑,
    sup = $MultiplicationOperator(
      symbol = '×',
      operands = [m, n]
    )
  ),
  definition = [$../"Right modules"(𝐑 = {𝐑}).module(
      M = {$../../../Matrices/Matrices(
        𝐑 = {𝐑},
        m = {m},
        n = {n}
      )},
      ⊕ = {$../../../../../Essentials/Operations/Operations(
          X = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )},
          Y = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )},
          Z = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )}
        ).operation(x = {
          #(A: %Element($../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          ))),
          {y = {
            #(B: %Element($../../../Matrices/Matrices(
              𝐑 = {𝐑},
              m = {m},
              n = {n}
            ))),
            {z = {$../../../Matrices/sum(
              𝐑 = {𝐑},
              m = {m},
              n = {n},
              A = {A},
              B = {B}
            )}}
          }}
      })},
      "0" = {$../../../Matrices/zero(
        𝐑 = {𝐑},
        m = {m},
        n = {n}
      )},
      ⊖ = {$../../../../../Essentials/Functions/Functions(
          X = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )},
          Y = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )}
        ).function(x = {
          #(A: %Element($../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          ))),
          {y = {$../../../Matrices/"additive inverse"(
            𝐑 = {𝐑},
            m = {m},
            n = {n},
            A = {A}
          )}}
      })},
      ⊙ = {$../../../../../Essentials/Operations/Operations(
          X = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )},
          Y = {$../../../Carrier(𝐑 = {𝐑})},
          Z = {$../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          )}
        ).operation(x = {
          #(A: %Element($../../../Matrices/Matrices(
            𝐑 = {𝐑},
            m = {m},
            n = {n}
          ))),
          {y = {
            #(r: %Element($../../../Carrier(𝐑 = {𝐑}))),
            {z = {$../../../Matrices/"product with right scalar"(
              𝐑 = {𝐑},
              m = {m},
              n = {n},
              A = {A},
              r = {r}
            )}}
          }}
      })}
  )]
}
