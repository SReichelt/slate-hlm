%../../../../../../logics/hlm%

$difference(
  ğ‘: %Element(
    $../../Rings,
    auto = %true
  ),
  ğŒ: %Element(
    $"Left modules"(ğ‘ = ğ‘),
    auto = %true
  ),
  x,y: %Element($Carrier(ğ‘ = ğ‘, ğŒ = ğŒ))
): %ExplicitOperator {
  notation = $SubtractionOperator(operands = [x, y]),
  definition = [$sum(
    ğ‘ = ğ‘, ğŒ = ğŒ, x = x,
    y = $"additive inverse"(ğ‘ = ğ‘, ğŒ = ğŒ, x = y)
  )]
}
