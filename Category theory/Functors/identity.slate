%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Definition:Identity_Functor
 *             * https://ncatlab.org/nlab/show/identity+functor
 *             * https://leanprover-community.github.io/mathlib_docs/category_theory/functor.html#category_theory.functor.id
 */
$identity(
  u: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  )
): %ExplicitOperator {
  notation = [$SubSup(
    body = 'id',
    sub = 𝓒
  )],
  definition = [$Functors(
      u = {u},
      v = {u},
      𝓒 = {𝓒},
      𝓓 = {𝓒}
    ).functor(
      𝐗 = {
        #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {𝐘 = {𝐗}}
      },
      𝐀 = {
        #(𝐀: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {𝐁 = {
          #(𝐁: %Element($../Objects(
            u = {u},
            𝓒 = {𝓒}
          ))),
          {f = {
            #(f: %Element($../Morphisms(
              u = {u},
              𝓒 = {𝓒},
              𝐗 = {𝐀},
              𝐘 = {𝐁}
            ))),
            {g = {f}}
          }}
        }}
      }
  )]
}
