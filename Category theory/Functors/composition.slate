%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Definition:Composition_of_Functors
 *             * https://leanprover-community.github.io/mathlib_docs/category_theory/functor.html#category_theory.functor.comp
 */
$composition(
  u: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  v: %Element(
    %previous,
    auto = %true
  ),
  w: %Element(
    %previous,
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    $../Categories(u = {v}),
    auto = %true
  ),
  𝓔: %Element(
    $../Categories(u = {w}),
    auto = %true
  ),
  F: %Element($Functors(
    u = {u},
    v = {v},
    𝓒 = {𝓒},
    𝓓 = {𝓓}
  )),
  G: %Element($Functors(
    u = {v},
    v = {w},
    𝓒 = {𝓓},
    𝓓 = {𝓔}
  ))
): %ExplicitOperator {
  display = [$Operator(
    symbol = '∘',
    operands = [G, F],
    minParenLevel = 2,
    maxParenLevel = 2
  )],
  definition = [$Functors(
      u = {u},
      v = {w},
      𝓒 = {𝓒},
      𝓓 = {𝓔}
    ).functor(
      𝐗 = {
        #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {𝐃 = {$"object value"(
          u = {v},
          v = {w},
          𝓒 = {𝓓},
          𝓓 = {𝓔},
          F = {G},
          𝐗 = {$"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )}
        )}}
      },
      𝐘 = {
        #(𝐘: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {𝐙 = {
          #(𝐙: %Element($../Objects(
            u = {u},
            𝓒 = {𝓒}
          ))),
          {f = {
            #(f: %Element($../Morphisms(
              u = {u},
              𝓒 = {𝓒},
              𝐗 = {𝐘},
              𝐘 = {𝐙}
            ))),
            {d = {$"morphism value"(
              u = {v},
              v = {w},
              𝓒 = {𝓓},
              𝓓 = {𝓔},
              F = {G},
              𝐘 = {$"object value"(
                u = {u},
                v = {v},
                𝓒 = {𝓒},
                𝓓 = {𝓓},
                F = {F},
                𝐗 = {𝐘}
              )},
              𝐙 = {$"object value"(
                u = {u},
                v = {v},
                𝓒 = {𝓒},
                𝓓 = {𝓓},
                F = {F},
                𝐗 = {𝐙}
              )},
              f = {$"morphism value"(
                u = {u},
                v = {v},
                𝓒 = {𝓒},
                𝓓 = {𝓓},
                F = {F},
                𝐘 = {𝐘},
                𝐙 = {𝐙},
                f = {f}
              )}
            )}}
          }}
        }}
      }
  )]
}
