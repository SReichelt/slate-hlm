%../../../../logics/hlm%

$identity(
  u: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    %previous,
    auto = %true
  ),
  F: %Element(
    $../Functors/Functors(
      u = {u},
      𝓒 = {𝓒},
      𝓓 = {𝓓}
    ),
    auto = %true
  )
): %ExplicitOperator {
  display = [$SubSup(
    body = 'id',
    sub = F
  )],
  definition = [
    $"Infranatural transformations"(
        u = {u},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(𝐗 = {
        parameter = #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        arguments = {n = {$../identity(
          u = {u},
          𝓒 = {𝓒},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )}
        )}}
    }),
    $"Infranatural transformations"(
        u = {u},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(𝐗 = {
        parameter = #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        arguments = {n = {$../Functors/"morphism value"(
          u = {u},
          𝓒 = {𝓒},
          𝓓 = {𝓓},
          F = {F},
          𝐘 = {$../Functors/"object value"(
            u = {u},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐙 = {$../Functors/"object value"(
            u = {u},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          f = {$../identity(
            u = {u},
            𝓒 = {𝓒},
            𝐗 = {𝐗}
          )}
        )}}
    })
  ]
}
