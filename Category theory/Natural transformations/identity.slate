%../../../../logics/hlm%

/**
 * @references * https://ncatlab.org/nlab/show/identity+natural+transformation
 *             * https://leanprover-community.github.io/mathlib_docs/category_theory/natural_transformation.html#category_theory.nat_trans.id
 */
$identity(
  u,v: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    $../Categories(u = {v}),
    auto = %true
  ),
  F: %Element(
    $../Functors/Functors(
      u = {u},
      v = {v},
      𝓒 = {𝓒},
      𝓓 = {𝓓}
    ),
    auto = %true
  )
): %ExplicitOperator {
  notation = $SubSup(
    body = 'id',
    sub = F
  ),
  definition = [
    $"Infranatural transformations"(
        u = {u},
        v = {v},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(_𝐗 = {
        #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {n = {$../identity(
          u = {v},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )}
        )}}
    }),
    $"Infranatural transformations"(
        u = {u},
        v = {v},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(_𝐗 = {
        #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        {n = {$../Functors/"morphism value"(
          u = {u},
          v = {v},
          𝓒 = {𝓒},
          𝓓 = {𝓓},
          F = {F},
          𝐗 = {𝐗},
          𝐘 = {𝐗},
          f = {$../identity(
            u = {u},
            𝓒 = {𝓒},
            𝐗 = {𝐗}
          )}
        )}}
    })
  ]
}
