%../../../../logics/hlm%

/**
 * @references https://leanprover-community.github.io/mathlib_docs/category_theory/natural_transformation.html#category_theory.nat_trans.app
 */
$component(
  u: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  v: %Element(
    %previous,
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    $../Categories(u = {v}),
    auto = %true
  ),
  F: %Element(
    $../Functors/Functors(
      u = {u},
      v = {v},
      𝓒 = {𝓒},
      𝓓 = {𝓓}
    ),
    auto = %true
  ),
  G: %Element(
    %previous,
    auto = %true
  ),
  η: %Element($"Infranatural transformations"(
    u = {u},
    v = {v},
    𝓒 = {𝓒},
    𝓓 = {𝓓},
    F = {F},
    G = {G}
  )),
  𝐗: %Element($../Objects(
    u = {u},
    𝓒 = {𝓒}
  ))
): %ExplicitOperator {
  notation = $SubSup(
    body = η,
    sub = 𝐗
  ),
  definition = [%structuralCases(
    η,
    $"Infranatural transformations"(
      u = {u},
      v = {v},
      𝓒 = {𝓒},
      𝓓 = {𝓓},
      F = {F},
      G = {G}
    ),
    [{
      constructor = $"Infranatural transformations".transformation,
      parameters = #(𝐘: %Binding(
        $../Objects(
          u = {u},
          𝓒 = {𝓒}
        ),
        #(n: %Element($../Morphisms(
          u = {v},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐘}
          )},
          𝐘 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐘}
          )}
        )))
      )),
      value = n[𝐗]
    }]
  )]
}
