%../../../../logics/hlm%

$component(
  u: %Element($../../Essentials/Numbers/Natural/"Natural numbers", auto = %true),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    %previous,
    auto = %true
  ),
  F: %Element(
    $../Functors/Functors(
      u = {u},
      𝓒 = {𝓒},
      𝓓 = {𝓓}
    ),
    auto = %true
  ),
  G: %Element(
    %previous,
    auto = %true
  ),
  η: %Element($"Infranatural transformations"(
    u = {u},
    𝓒 = {𝓒},
    𝓓 = {𝓓},
    F = {F},
    G = {G}
  )),
  𝐗: %Element($../Objects(
    u = {u},
    𝓒 = {𝓒}
  ))
): %ExplicitOperator {
  display = [$SubSup(
    body = η,
    sub = 𝐗
  )],
  definition = [%structuralCases(
    η,
    $"Infranatural transformations"(
      u = {u},
      𝓒 = {𝓒},
      𝓓 = {𝓓},
      F = {F},
      G = {G}
    ),
    [{
      constructor = $"Infranatural transformations"(
          u = {u},
          𝓒 = {𝓒},
          𝓓 = {𝓓},
          F = {F},
          G = {G}
        ).transformation,
      parameters = #(𝐘: %Binding(
        $../Objects(
          u = {u},
          𝓒 = {𝓒}
        ),
        #(n: %Element($../Morphisms(
          u = {u},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐘}
          )},
          𝐘 = {$../Functors/"object value"(
            u = {u},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐘}
          )}
        )))
      )),
      value = n[𝐗]
    }]
  )]
}
