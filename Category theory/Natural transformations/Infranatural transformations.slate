%../../../../logics/hlm%

/**
 * @remarks Here, we first define infranatural transformations, as a prerequisite for defining natural transformations (see `$"Natural transformations"`). This is necessary if we want to be able to ask whether a given transformation is natural or not.
 *
 * @references * https://en.wikipedia.org/wiki/Natural_transformation#Definition: infranatural transformation
 */
$"Infranatural transformations"(
  u: %Element(
    $../../Essentials/Numbers/Natural/"Natural numbers",
    auto = %true
  ),
  v: %Element(
    %previous,
    auto = %true
  ),
  𝓒: %Element(
    $../Categories(u = {u}),
    auto = %true
  ),
  𝓓: %Element(
    $../Categories(u = {v}),
    auto = %true
  ),
  F: %Element($../Functors/Functors(
    u = {u},
    v = {v},
    𝓒 = {𝓒},
    𝓓 = {𝓓}
  )),
  G: %Element(%previous)
): %Construction {
  $transformation(𝐗: %Binding(
    $../Objects(
      u = {u},
      𝓒 = {𝓒}
    ),
    #(n: %Element($../Morphisms(
      u = {v},
      𝓒 = {𝓓},
      𝐗 = {$../Functors/"object value"(
        u = {u},
        v = {v},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        𝐗 = {𝐗}
      )},
      𝐘 = {$../Functors/"object value"(
        u = {u},
        v = {v},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {G},
        𝐗 = {𝐗}
      )}
    )))
  )): %Constructor {
    notation = [$SubSup(
      body = $Parens(
        body = n,
        style = '()'
      ),
      sub = $ElementRelation(
        element = 𝐗,
        set = 𝓒
      )
    )],
    equalityDefinition = {
      leftParameters = #(𝐗: %Binding(
        $../Objects(
          u = {u},
          𝓒 = {𝓒}
        ),
        #(n: %Element($../Morphisms(
          u = {v},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐘 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐗}
          )}
        )))
      )),
      rightParameters = #(𝐗: %Binding(
        $../Objects(
          u = {u},
          𝓒 = {𝓒}
        ),
        #(m: %Element($../Morphisms(
          u = {v},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐘 = {$../Functors/"object value"(
            u = {u},
            v = {v},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐗}
          )}
        )))
      )),
      definition = [%forall(
        #(𝐗: %Element($../Objects(
          u = {u},
          𝓒 = {𝓒}
        ))),
        %equals(n[𝐗], m[𝐗])
      )],
      isomorphic = %true
    }
  }

  notation = [$Function(
    function = 'Infranat',
    arguments = [F, G]
  )],
  definitionNotation = {
    parameter = #(η: %Expr),
    notation = [$FunctionDefinition(
      function = η,
      symbol = '⇀',
      operands = [F, G]
    )],
    singularName = 'infranatural transformation',
    pluralName = 'infranatural transformations',
    nameOptional = %true
  }
}
