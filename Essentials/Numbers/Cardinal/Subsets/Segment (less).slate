$"Segment (less)"(κ: %Element($../"Cardinal numbers")): %SetOperator {
  notation = $Segment(
    set = '𝓒𝓻𝓭',
    symbol = '<',
    limit = κ
  ),
  definition = [%subset(
    #(μ: %Element($../"Cardinal numbers")),
    $../less(
      κ = {μ},
      μ = {κ}
    )
  )]
}
