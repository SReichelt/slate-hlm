%../../../../../logics/hlm%

$finite(κ: %Element($"Cardinal numbers")): %Predicate {
  display = [$Property(
    operand = κ,
    property = %neg('finite', 'infinite')
  )],
  definition = [
    $../../Sets/finite(S = {$Carrier(κ = {κ})}),
    %in(
      κ,
      $../Natural/"Natural numbers"
    )
  ]
}
