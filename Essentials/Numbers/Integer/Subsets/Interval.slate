%../../../../../../logics/hlm%

$Interval(
  a: %Element($../Integers),
  b: %Element(%previous)
): %SetOperator {
  notation = $Enumeration(items = [
    a,
    $BottomEllipsis,
    b
  ]),
  definition = [%subset(
    #(c: %Element($../Integers)),
    %and(
      $../"less or equal"(
        a = {a},
        b = {c}
      ),
      $../"less or equal"(
        a = {c},
        b = {b}
      )
    )
  )]
}
