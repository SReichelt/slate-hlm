%../../../../../logics/hlm%

/**
 * @references * https://leanprover-community.github.io/mathlib_docs/core/init/data/nat/lemmas.html#nat.lt_succ_self
 */
$"Successor is always greater"(n: %Element($"Natural numbers")): %StandardTheorem {
  claim = $less(
    n = $sum(
      m = n,
      n = $number(value = 1)
    ),
    m = n
  )
}
