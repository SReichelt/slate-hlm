%../../../../../logics/hlm%

$"Multiplication by a positive number is injective"(
  a,b: %Element($"Natural numbers"),
  c: %Element($Subsets/"Positive numbers"),
  _1: %Constraint(%equals(
    $product(
      m = a,
      n = c
    ),
    $product(
      m = b,
      n = c
    )
  ))
): %StandardTheorem {
  claim = %equals(a, b)
}
