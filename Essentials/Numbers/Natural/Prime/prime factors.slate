%../../../../../../logics/hlm%

$"prime factors"(n: %Element($../"Natural numbers")): %ExplicitOperator {
  notation = $Function(
    function = 'PrimeFactors',
    arguments = [n]
  ),
  definition = [$../../../Sets/Multisets/Multisets(S = $"Prime numbers").multiset(_1 = {
      #(p: %Element($"Prime numbers")),
      {m = $multiplicity(
        n = n,
        p = p
      )}
  })]
}
