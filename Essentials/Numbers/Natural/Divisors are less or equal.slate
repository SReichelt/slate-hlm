%../../../../../logics/hlm%

/**
 * @references * https://leanprover-community.github.io/mathlib_docs/core/init/data/nat/lemmas.html#nat.le_of_dvd
 */
$"Divisors are less or equal"(
  m,n: %Element($Subsets/"Positive numbers"),
  _1: %Constraint($divisible(
    m = {m},
    n = {n}
  ))
): %StandardTheorem {
  claim = $"less or equal"(
    m = {m},
    n = {n}
  )
}
