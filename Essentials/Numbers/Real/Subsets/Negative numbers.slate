%../../../../../../logics/hlm%

$"Negative numbers": %SetOperator {
  notation = $SubSup(
    body = 'ℝ',
    sub = '−'
  ),
  definition = [
    $"Segment (less)"(a = {$../../Natural/number(value = 0)}),
    %subset(
      #(a: %Element($../"Real numbers")),
      $../less(
        a = {a},
        b = {$../../Natural/number(value = 0)}
      )
    )
  ]
}
