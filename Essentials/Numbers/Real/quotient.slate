%../../../../../logics/hlm%

$quotient(
  a: %Element($"Real numbers"),
  b: %Element($Subsets/"Nonzero numbers")
): %ImplicitOperator {
  notation = [$Fraction(
    numerator = a,
    denominator = b
  )],
  parameter = #(x: %Element($"Real numbers")),
  definition = [%equals(
    a,
    $product(
      a = {x},
      b = {b}
    )
  )]
}
