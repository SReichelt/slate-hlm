%../../../../../../logics/hlm%

$"Nonzero numbers": %SetOperator {
  notation = $SubSup(
    body = 'ℂ',
    sup = '×'
  ),
  definition = [
    $Hole(a = $../../Natural/number(value = 0)),
    %subset(
      #(a: %Element($../"Complex numbers")),
      %not(%equals(
        a,
        $../../Natural/number(value = 0)
      ))
    )
  ]
}
