%../../../../../logics/hlm%

$countable(α: %Element($"Ordinal numbers")): %Predicate {
  notation = $Property(
    operand = α,
    property = %neg('countable', 'uncountable')
  ),
  definition = [
    $../Cardinal/countable(κ = {$cardinality(α = {α})}),
    $../../Sets/countable(S = {$Carrier(α = {α})})
  ]
}
