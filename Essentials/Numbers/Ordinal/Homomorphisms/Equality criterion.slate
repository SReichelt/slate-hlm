%../../../../../../logics/hlm%

$"Equality criterion"(
  α: %Element($../"Ordinal numbers"),
  β: %Element(%previous)
): %EquivalenceTheorem {
  conditions = [
    %and(
      $../"less or equal"(
        α = {α},
        β = {β}
      ),
      $../"less or equal"(
        α = {β},
        β = {α}
      )
    ),
    %not($../../../Sets/empty(S = {$Isomorphisms(
      α = {α},
      β = {β}
    )})),
    %equals(α, β)
  ]
}
