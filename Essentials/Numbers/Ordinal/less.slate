%../../../../../logics/hlm%

$less(
  α: %Element($"Ordinal numbers"),
  β: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('<', '≮'),
      operands = [α, β]
    ),
    $Relation(
      symbol = %neg('>', '≯'),
      operands = [β, α]
    )
  ],
  definition = [%and(
    $"less or equal"(
      α = {α},
      β = {β}
    ),
    %not(%equals(α, β))
  )]
}
