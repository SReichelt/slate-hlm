%../../../../../logics/hlm%

/**
 * @references * https://leanprover-community.github.io/mathlib_docs/set_theory/ordinal.html#ordinal.lt
 */
$less(α,β: %Element($"Ordinal numbers")): %Predicate {
  notation = $LessThanRelation(operands = [α, β]),
  definition = [%and(
    $"less or equal"(
      α = α,
      β = β
    ),
    %not(%equals(α, β))
  )]
}
