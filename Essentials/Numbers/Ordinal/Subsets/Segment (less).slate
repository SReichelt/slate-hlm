%../../../../../../logics/hlm%

$"Segment (less)"(α: %Element($../"Ordinal numbers")): %SetOperator {
  display = [$Segment(
    set = '𝓞𝓻𝓭',
    symbol = '<',
    limit = α
  )],
  definition = [%subset(
    #(β: %Element($../"Ordinal numbers")),
    $../less(
      α = {β},
      β = {α}
    )
  )]
}
