%../../../../../logics/hlm%

$"less or equal"(
  𝐱: %Element($"Ordinal numbers"),
  𝐲: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('≤', '≰'),
      operands = [𝐱, 𝐲]
    ),
    $Relation(
      symbol = %neg('≥', '≱'),
      operands = [𝐲, 𝐱]
    )
  ],
  definition = [
    %exists(
      #(φ: %Element($Homomorphisms/Homomorphisms(
        𝐱 = {𝐱},
        𝐲 = {𝐲}
      ))),
      formula = $../../Functions/injective(
        X = {$Carrier(𝐱 = {𝐱})},
        Y = {$Carrier(𝐱 = {𝐲})},
        f = {φ}
      )
    ),
    %exists(
      #(S: %Subset($Carrier(𝐱 = {𝐲}))),
      formula = %and(
        $Subsets/"ordered subset"(
          𝐱 = {𝐲},
          S = {S}
        ),
        %equals(
          𝐱,
          $Subsets/"subset ordinal"(
            𝐱 = {𝐲},
            S = {S}
          )
        )
      )
    )
  ]
}
