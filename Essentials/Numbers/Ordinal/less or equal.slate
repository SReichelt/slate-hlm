%../../../../../logics/hlm%

$"less or equal"(
  𝐱: %Element($"Ordinal numbers"),
  𝐲: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('≤', '≰'),
      operands = [𝐱, 𝐲]
    ),
    $Relation(
      symbol = %neg('≥', '≱'),
      operands = [𝐲, 𝐱]
    )
  ],
  definition = [%exists(
    #(φ: %Element($Homomorphisms(
      𝐱 = {𝐱},
      𝐲 = {𝐲}
    ))),
    formula = $../../Functions/injective(
      X = {$Carrier(𝐱 = {𝐱})},
      Y = {$Carrier(𝐱 = {𝐲})},
      f = {φ}
    )
  )]
}
