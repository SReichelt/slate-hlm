%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Union_is_Commutative
 *             * http://us.metamath.org/mpeuni/uncom.html
 *             * https://leanprover-community.github.io/mathlib_docs/data/set/basic.html#set.union_comm
 */
$"Union is commutative"(
  U: %Set,
  S,T: %Subset(U)
): %StandardTheorem {
  claim = %setEquals(
    $Union(U = U, S = S, T = T),
    $Union(U = U, S = T, T = S)
  )
}
