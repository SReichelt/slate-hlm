%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Intersection_is_Subset
 *             * https://leanprover-community.github.io/mathlib_docs/data/set/basic.html#set.inter_subset_left
 */
$"Intersection is subset of argument"(
  U: %Set,
  S: %Subset(U),
  T: %Subset(%previous)
): %StandardTheorem {
  claim = %sub(
    $Intersection(
      U = {U},
      S = {S},
      T = {T}
    ),
    S
  )
}
