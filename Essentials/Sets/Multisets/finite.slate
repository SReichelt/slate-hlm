%../../../../../logics/hlm%

$finite(
  S: %Set(auto = %true),
  𝑻: %Element($Multisets(S = {S}))
): %Predicate {
  notation = $Property(
    operand = 𝑻,
    property = %neg('finite', 'infinite')
  ),
  definition = [
    $../finite(S = {$Elements(
      S = {S},
      𝑻 = {𝑻}
    )}),
    $../finite(S = {$Set(
      S = {S},
      𝑻 = {𝑻}
    )})
  ]
}
