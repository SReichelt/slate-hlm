%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Union_is_Associative
 *             * https://leanprover-community.github.io/mathlib_docs/data/set/basic.html#set.union_assoc
 */
$"Union is associative"(
  U: %Set,
  R,S,T: %Subset(U)
): %StandardTheorem {
  claim = %setEquals(
    $Union(
      U = U,
      S = $Union(U = U, S = R, T = S),
      T = T
    ),
    $Union(
      U = U,
      S = R,
      T = $Union(U = U, S = S, T = T)
    )
  )
}
